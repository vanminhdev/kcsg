{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "KCS_DATA": "Server=.\\SQLEXPRESS;Database=KCS_DATA;User ID=sa;Password=1234567"
  },
  "UseUrls": [
    "http://*:5000"
  ],
  "Log4NetCore": {
    "Name": "Test",
    "LoggerRepository": "Fantastic",
    "OverrideCriticalLevelWith": "Fatal",
    "Watch": false,
    "UseWebOrAppConfig": false,
    "PropertyOverrides": [
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file",
        "Attributes": {
          "Value": "kcs.log"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/maximumFileSize",
        "Attributes": {
          "Value": "500KB"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file"
      }
    ]
  }
